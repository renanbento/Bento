#script para o zoom

set palette defined (0 0 0 0.9, 1 0 0 1, 2 0 0.5 1, 3 0 1 1, 4 0.5 1 0.5, 5 1 1 0, 6 1 0.5 0, 7 1 0 0, 8 0.9 0 0)
set lmargin at screen 0.10
set rmargin at screen 0.90
set bmargin at screen 0.30
set tmargin at screen 0.70
set grid front ls 8
set yrange [ -0.01 : 0.01 ]
set tics font ' ,10'
set cbtics ('1' 1, '0' 0, '-1' -1)
set cblabel ('') font ' ,30' offset 3,0,0
set xrange [ -1.5 : 1.5 ]
set ytics (-0.01, 0 , 0.01)
set xtics (-1.23,-0.73, -0.55,-0.05, ' ' 0.05)
set ylabel "{/: E/t }" font 'Linux Biolinum O,15'  offset 3,0,0
set xlabel "{/Symbol m}"  font 'Linux Biolinum O,15'  offset 0,0,0
plot'/home/grunge/Dropbox/Programa zheev/NANO2/N100.dat' u 1:2:3  w points pointtype 7 ps 0.6 lw 2 palette t " "
set terminal pdf
set output "N80zoomfinal.pdf"
plot'/home/bento/Dropbox/Programa zheev/NANO2/N100.dat' u 1:2:3  w points pointtype 7 ps 0.2 lw 2 palette t " "
set output 



