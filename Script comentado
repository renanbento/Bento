#script para o zoom

set palette defined (0 0 0 0.9, 1 0 0 1, 2 0 0.5 1, 3 0 1 1, 4 0.5 1 0.5, 5 1 1 0, 6 1 0.5 0, 7 1 0 0, 8 0.9 0 0)
set lmargin at screen 0.10
set rmargin at screen 0.90
set bmargin at screen 0.30
set tmargin at screen 0.70
set ytics (-0.5, 0 , 0.5)
set grid front ls 7
set y2tics (0.2)
set xtics (-1.2,-0.6 ,-0.3) 
set xtics add  ('1' -1,'2' -0.6 ,'1' -0.3, 0) offset -0.4,15.3,0 


set grid front ls 8
set lmargin at screen 0.10
set rmargin at screen 0.90
set bmargin at screen 0.15
set tmargin at screen 0.85
set xtics ('-1.28'-1.25,'-0.78'-0.76,'-0.5' -0.52 ,-0.02,'1.28'1.25,'0.78'0.76,'0.5' 0.52 ) rotate by 45 right
set yrange [ -0.5 : 0.5 ]
set tics font 'calibri ,25'
set xrange [ -1.5 : 1.5 ]
set yrange [ -0.5 : 0.5 ]
set ylabel "{/: E/t }" font 'calibri O,25'  offset 3,0,0
set xlabel "{/Symbol m}"  font 'calibri O,25'  offset 0,0.5,0
